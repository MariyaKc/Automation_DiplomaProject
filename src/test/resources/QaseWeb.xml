<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="QASE Web tests" thread-count="2" parallel="tests">

    <test name=" Login tests set" >
        <classes>
            <class name="WEB_Tests.Login_Test"/>
        </classes>
    </test>

    <test name=" Sign Up tests set" >
        <classes>
            <class name="WEB_Tests.SignUp_Test"/>
        </classes>
    </test>

    <test name=" Dialog window tests set"  >
        <classes>
            <class name="WEB_Tests.DialogWindow_Test"/>
        </classes>
    </test>

    <test name=" Pop Up tests set" >
        <classes>
            <class name="WEB_Tests.PopUpFrame_Test"/>
        </classes>
    </test>

    <test name=" Repository upload and download tests set"  >
        <classes>
            <class name="WEB_Tests.Repository_Test"/>
        </classes>
    </test>

    <test name=" Create Project tests set" >
        <classes>
            <class name="WEB_Tests.Projects_Test"/>
        </classes>
    </test>

</suite>