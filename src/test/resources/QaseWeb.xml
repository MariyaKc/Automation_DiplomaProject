<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="QASE Web tests" thread-count="6" parallel="tests">

    <test name=" Login tests set" >
        <classes>
            <class name="WEB_Tests.Login_Test"/>
        </classes>
    </test>

    <test name=" Sign Up tests set" >
        <classes>
            <class name="WEB_Tests.SignUp_Test"/>
        </classes>
    </test>

    <test name=" Dialog window tests set"  >
        <classes>
            <class name="WEB_Tests.DialogWindow_Test"/>
        </classes>
    </test>

    <test name=" Pop Up tests set" >
        <classes>
            <class name="WEB_Tests.PopUpFrame_Test"/>
        </classes>
    </test>

    <test name=" Repository upload and download tests set"  >
        <classes>
            <class name="WEB_Tests.Repository_Test"/>
        </classes>
    </test>

    <test name=" Create and Delete Project tests set" >
        <classes>
            <class name="WEB_Tests.Projects_Test"/>
        </classes>
    </test>

    <test name=" Boundary value tests set" enabled="false" >
        <classes>
            <class name="WEB_Tests.BoundaryValue_Test"/>
        </classes>
    </test>

    <test name=" Create and Delete Test Case tests set" >
        <classes>
            <class name="WEB_Tests.Case_Test"/>
        </classes>
    </test>

    <test name=" Create and Delete Suite tests set" >
        <classes>
            <class name="WEB_Tests.Suite_Test"/>
        </classes>
    </test>

    <test name=" Create and Delete Defect tests set" >
        <classes>
            <class name="WEB_Tests.Defect_Test"/>
        </classes>
    </test>

    <test name="Create, sort and delete Milestones tests" >
        <classes>
            <class name="WEB_Tests.Milestone_Test"/>
        </classes>
    </test>

</suite>